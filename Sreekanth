import xlsxwriter 
import csv
def avgfinder(batsmen,bowler):
    with open('C:\\Users\\MATRA\\OneDrive\\Desktop\\all matches -1.csv', mode='r') as csv_file:
        csv_reader = csv.reader(csv_file)
        line_count = 0
        dummy=0
        ave=0
        for row in csv_reader:
            if line_count == 0:
                pass
                line_count += 1
            if row[8]==batsmen:
                if row[10]==bowler:
                    ave=ave+int(row[11])
                    dummy=dummy+1

            line_count += 1
        return(ave/dummy)
        
avgfinder("V Kohli","JO Holder")
1.1666666666666667
def final_file(batsmen,bowlers):
    for i in batsmen:
        file=i+".xlsx"
        workbook = xlsxwriter.Workbook(file)
        worksheet = workbook.add_worksheet()
        for j in bowlers:
            row=0
            col=0
            try:
                runs = [j,avgfinder(i,j)]
                worksheet.write(row, col, runs[0])
                worksheet.write(row, col + 1, runs[1])
            except ZeroDivisionError:
                runs = [j,0.6]
                worksheet.write(row, col, runs[0])
                worksheet.write(row, col + 1, runs[1])
                
            row=row+1
        workbook.close()
    print("done")
            
