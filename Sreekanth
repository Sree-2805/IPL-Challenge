import numpy as np
import matplotlib.pyplot as plt
import pandas as pd
import statsmodels.api as sm
from sklearn.preprocessing import PolynomialFeatures
import xlrd
b=[]
workbook = xlrd.open_workbook("C:\\Users\\MATRA\\OneDrive\\Desktop\\bowlers.xlsx")
sheet = workbook.sheet_by_index(0)
data = [sheet.row_values(rowx) for rowx in range(sheet.nrows)]

for i in range(len(data)):
    b.append(float(data[i][1])/10)
    
polynomial_features= PolynomialFeatures(degree=60)
ba=np.array(b)
bp=ba.reshape(-1,1)
xp = polynomial_features.fit_transform(bp)

bat=[]
file="RG Sharma"+".xlsx"
workbook = xlrd.open_workbook("C:\\Users\\MATRA\\"+file)
sheet = workbook.sheet_by_index(0)
data = [sheet.row_values(rowx) for rowx in range(sheet.nrows)]

for i in range(len(data)):
    bat.append(float(data[i][1]))

model = sm.OLS(bat, xp).fit()
ypred = model.predict(xp) 

plt.scatter(b,bat)
plt.plot(b,ypred)
a=model.params
dic={"RG Sharma":a}

def finalfun(batsman):
    b=[]
    workbook = xlrd.open_workbook("C:\\Users\\MATRA\\OneDrive\\Desktop\\bowlers.xlsx")
    sheet = workbook.sheet_by_index(0)
    data = [sheet.row_values(rowx) for rowx in range(sheet.nrows)]
    for i in range(len(data)):
        b.append(float(data[i][1])/10)
    bat=[]
    file=batsman+".xlsx"
    workbook = xlrd.open_workbook("C:\\Users\\MATRA\\"+file)
    sheet = workbook.sheet_by_index(0)
    data = [sheet.row_values(rowx) for rowx in range(sheet.nrows)]
    for i in range(len(data)):
        bat.append(float(data[i][1]))
    
    polynomial_features= PolynomialFeatures(degree=60)
    ba=np.array(b)
    bp=ba.reshape(-1,1)
    xp = polynomial_features.fit_transform(bp)
    
    model = sm.OLS(bat, xp).fit()
    ypred = model.predict(xp) 
    
    a=model.params
    return a
final_dic={}
for i in batsmen:
    final_dic[i]=finalfun(i)
