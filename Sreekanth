    No_of_Wickets=abs(len(Batsmen)-2)
    no_of_balls=36-(No_of_Wickets)*2
    bowled_balls=no_of_balls/(len(Batsmen)*len(Bowlers))
    final_score=0
    for i in Batsmen:
        score_per_bowler=0
        for j in Bowlers:
            try:
                a=bowl_dic[j]/10
            except KeyError:
                a=7.792/10
            score_per_ball=0
            for k in range(61):
                try:
                    score_per_ball=score_per_ball+(bat_dic[i][k]*a**k)
                except KeyError:
                    score_per_ball=score_per_ball+(bat_dic["D Padikkal"][k]*a**k)
            score_per_bowler=score_per_ball*bowled_balls
            final_score=final_score+score_per_bowler
    final_SCORE=(final_score*100/92)*(Innings-1)+(final_score*100/85)*(2-Innings)
    if BowlingTeam=="Mumbai Indians" or BowlingTeam=="Sunrisers Hyderabad":
        final_SCORE=final_SCORE-final_SCORE*0.04
    if BattingTeam=="Royal Challengers Bangalore" : 
        final_SCORE=final_SCORE+final_SCORE*0.08
    if BattingTeam=="Delhi Capitals":
         final_SCORE=final_SCORE+final_SCORE*0.15
    if BattingTeam=="Mumbai Indians":
         final_SCORE=final_SCORE+final_SCORE*0.04
    if Venue=="Arun Jaitley Stadium" or Venue=="Narendra Modi Stadium":
         final_SCORE=final_SCORE+final_SCORE*0.04
    if Venue=="ChinnaSwamy Stadium":
         final_SCORE=final_SCORE+final_SCORE*0.20
    if BowlingTeam=="Rajasthan Royals":
        return int(final_score)
    else:
        return round(final_SCORE)
